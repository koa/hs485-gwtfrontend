<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
  ui:generateLocales="default, de">
  <ui:style>
    
  </ui:style>
  <g:SplitLayoutPanel width="1000" height="700">
    <g:west size="325.0">
      <g:VerticalPanel ui:field="addInputDeviceButton">
        <g:HorizontalPanel />
        <g:Grid>
          <g:row>
            <g:customCell>
              <g:ListBox visibleItemCount="1" ui:field="selectFloorListBox" />
            </g:customCell>
            <g:customCell>
              <g:Button ui:field="addFloorButton">
                <ui:msg description="text for add Floor button">add Floor</ui:msg>
                ...
              </g:Button>
            </g:customCell>
          </g:row>
          <g:row>
            <g:cell>
            </g:cell>
            <g:customCell>
              <g:Button ui:field="removeFloorButton">
                <ui:msg description="text for remove Floor button">remove current Floor</ui:msg>
              </g:Button>
            </g:customCell>
          </g:row>
          <g:row>
            <g:cell></g:cell>
            <g:customCell>
              <g:HorizontalPanel width="100%"
                horizontalAlignment="ALIGN_CENTER">
                <g:Button ui:field="incScaleButton">+</g:Button>
                <g:Button ui:field="decScaleButton">-</g:Button>
              </g:HorizontalPanel>
            </g:customCell>
          </g:row>
          <g:row />
          <g:row>
            <g:customCell>
              <g:ListBox visibleItemCount="1" ui:field="selectFileListBox" />
            </g:customCell>
            <g:customCell>
              <g:Button ui:field="addFileButton">
                <ui:msg description="text for add File button">add File</ui:msg>
                ...
              </g:Button>
            </g:customCell>
          </g:row>
          <g:row>
            <g:customCell>
              <g:Button ui:field="outputDeviceButton">
                <ui:msg description="text for add Outputdevice button">add Outputdevice</ui:msg>
              </g:Button>
            </g:customCell>
            <g:customCell>
              <g:Button ui:field="inputDeviceButton">
                <ui:msg description="text for add Inputdevice button">add Inputdevice</ui:msg>
              </g:Button>
            </g:customCell>
          </g:row>
        </g:Grid>
      </g:VerticalPanel>
    </g:west>
    <g:center>
      <g:HTMLPanel ui:field="svgPanel" />
    </g:center>
  </g:SplitLayoutPanel>
</ui:UiBinder> 