<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    
  </ui:style>
  <g:Grid>
  	<g:row>
  		<g:customCell>
  			<g:Label text="Name"/>
  		</g:customCell>
  		<g:customCell>
  			<g:TextBox ui:field="nameTextInput"/>
  		</g:customCell>
  	</g:row>
  	<g:row>
  		<g:customCell>
  			<g:Label text="Type"/>
  		</g:customCell>
  		<g:customCell>
  			<g:ListBox visibleItemCount="1" ui:field="typeListBox"/>
  		</g:customCell>
  	</g:row>
  	<g:row>
  		<g:cell></g:cell>
  		<g:customCell>
  			<g:HorizontalPanel horizontalAlignment="ALIGN_RIGHT" >
  				<g:Button ui:field="cancelButton">cancel</g:Button>
  					<g:Button ui:field="saveButton">save</g:Button>
  			</g:HorizontalPanel>
  		</g:customCell>
  	</g:row>
  	<g:row/>
  </g:Grid>
</ui:UiBinder> 